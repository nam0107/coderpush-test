{
  "name": "tinder-service",
  "version": "1.0.0",
  "description": "tinder",
  "main": "./src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "make bump-version",
    "postrelease": "git push origin HEAD --tags --no-verify",
    "lint": "eslint ./src/**/*.ts",
    "typecheck": "npx tsc --project tsconfig.json --noEmit --skipLibCheck",
    "migration:create": "sequelize migration:generate --name",
    "migration:status": "sequelize db:migrate:status",
    "up": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "down": "sequelize db:migrate:undo:all",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.json && cp src/apiDocs.yaml dist/",
    "prestart": "npm run migration:up",
    "start": "node dist/server.js",
    "start:dev": "make start-dev",
    "debug": "make debug-ts",
    "prepare": "husky install",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts --verbose"
  },
  "husky": {
    "hooks": {
      "pre-coomit": "npm run lint & npm run typecheck",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "bnpl"
  ],
  "author": "nam",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@faker-js/faker": "^6.2.0",
    "@types/aws-sdk": "2.7.0",
    "@types/bluebird": "3.5.36",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/dotenv-safe": "8.1.2",
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/swagger-ui-express": "4.1.3",
    "@types/winston": "2.4.4",
    "@types/yamljs": "0.2.31",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint": "8.4.1",
    "husky": "7.0.4",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@types/multer": "1.4.7",
    "aws-sdk": "2.1048.0",
    "bluebird": "3.7.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "csv-parser": "3.0.0",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "joi": "17.5.0",
    "multer": "1.4.4",
    "nocache": "3.0.1",
    "pg": "8.7.1",
    "sequelize": "6.12.0",
    "sequelize-cli": "6.3.0",
    "swagger-ui-express": "4.3.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "winston": "3.3.3",
    "yamljs": "0.3.0",
    "yarn": "1.22.17"
  }
}
